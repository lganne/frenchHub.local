{# empty Twig template #}
{% extends 'base.html.twig' %}
{% block stylesheets %}
       {{parent()}}| <link rel="stylesheet" type="text/css" href="css/formulaire.css " />          
{% endblock %}
{% block body %}
    
    <div class="content standard" >
         <a href="/accueil" id="logo_top"><img src="../img/logo-french.png" alt="Logo de MyFrenchHub"></a>
    </div>
    {% if (message3 !=null ) %}
          <span class="mess"> {{ message3 }} </span>
    {% endif %}
    
 <section class="milieu" >
     <h1>Etre recontacté</h1><br>
            <form method='post' action='/contact'>
                    <label for='societe'>Dénomination :</label>  <input type='text' name='societe' required><br>
                    <label for="contact">Contact principal</label> :   <input type="text" name="contact" required>   <br>
                    <label for="fonction">Fonction</label> :   <input type="text" name="fonction" required>  <br> 
                    <label for="email">Email</label> :   <input type="email" name="email" required >   <br>
                    <label for="pays">pays</label> :   
                        <select name="select">
                            {% for pays in nomPays %}
                                            <option value="value1">{{pays.nom_fr_fr}}</option> 
                            {% endfor%}
                        </select><br>
                        <label for="Tel">Tel</label> :   <input type="tel" name="tel" required >   <br><br>
                    <input type="submit" name="valider" value="valider">
               
                </form>
                        
       
  </section>
                        
       {% endblock%}
                            
